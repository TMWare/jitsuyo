{
  "name": "@tmware/jitsuyo",
  "version": "1.4.1",
  "description": "Utility package for tmware projects",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "yarn run clean && tsc -p ./",
    "clean": "rimraf dist",
    "test": "jest --config jestconfig.json",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore src/",
    "lint:fix": "yarn run lint --fix",
    "preversion": "yarn run build && yarn test",
    "prepublishOnly": "yarn run build && yarn test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMWare/jitsuyo.git"
  },
  "author": "TMUniversal <me@tmuniversal.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TMWare/jitsuyo/issues"
  },
  "homepage": "https://github.com/TMWare/jitsuyo#readme",
  "dependencies": {
    "acronymresolver": "^1.1.2"
  },
  "devDependencies": {
    "@tmuniversal/ts": "^1.1.1",
    "@tmware/eslint-config": "^1.0.0",
    "@tmware/prettier-config": "^1.0.1",
    "@types/node": "^14.14.19",
    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:fix && yarn run lint",
      "pre-push": "yarn run test"
    }
  },
  "keywords": [],
  "prettier": "@tmware/prettier-config"
}
